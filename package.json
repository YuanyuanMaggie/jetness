{
  "name": "jetness",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.1",
    "npm": "3.10.10"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "zip-node": "rm -rf deploy-node.zip && zip -r deploy-node.zip index.js Web.config package.json node_modules client/build",
    "zip-static": "rm -rf deploy-static.zip && cd client/build && zip -r ../../deploy-static.zip *",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --only=dev && npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuanyuanMaggie/jetness.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YuanyuanMaggie/jetness/issues"
  },
  "homepage": "https://github.com/YuanyuanMaggie/jetness#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "nodemon": "^1.14.12",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2"
  }
}
